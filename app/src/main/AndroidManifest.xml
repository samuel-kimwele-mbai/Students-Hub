<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StudentsHub"
        tools:targetApi="31">
        <activity android:name=".AboutActivity" />
        <activity android:name=".CommunityActivity" />
        <activity android:name=".EventsActivity" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".HelpActivity" />
        <activity android:name=".ResourcesActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".AdminDashboardActivity" />
        <activity android:name=".AddEventsActivity"/>
        <activity android:name=".AddResourcesActivity"/>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>